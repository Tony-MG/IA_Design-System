  "Componenets":{
    "Primary-button": {
    "id": "btn-action-v1",
    "name": "Bouton d’action (Tokenisé DS)",
    "type": "button",
    "description": "Un bouton d’action principal, accessible et stylé via les tokens du Design System (couleurs, typo, rayons, espacements).",
    "properties": [
      {
        "name": "label",
        "type": "string",
        "description": "Texte visible du bouton.",
        "example": "Appeler l’action"
      },
      {
        "name": "ariaLabel",
        "type": "string",
        "description": "Libellé ARIA pour l’accessibilité. Par défaut, identique à label.",
        "example": "Appeler l’action"
      },
      {
        "name": "type",
        "type": "string",
        "description": "Type natif du bouton.",
        "example": "button"
      },
      {
        "name": "iconLeft",
        "type": "string",
        "description": "Nom d’icône ou SVG inline placé à gauche (vide si aucune icône).",
        "example": "<svg aria-hidden=\"true\" viewBox=\"0 0 20 20\"></svg>"
      },
      {
        "name": "iconRight",
        "type": "string",
        "description": "Nom d’icône ou SVG inline placé à droite (vide si aucune icône).",
        "example": ""
      },
      {
        "name": "variant",
        "type": "string",
        "description": "Style visuel du bouton. Exemples: primary (plein), secondary (contour), ghost (texte).",
        "example": "primary"
      },
      {
        "name": "size",
        "type": "string",
        "description": "Taille du bouton influençant le padding et la taille de police. Exemples: sm | md | lg.",
        "example": "md"
      },
      {
        "name": "fullWidth",
        "type": "boolean",
        "description": "Si vrai, le bouton occupe toute la largeur disponible.",
        "value": false
      },
      {
        "name": "disabled",
        "type": "boolean",
        "description": "Si vrai, le bouton est désactivé et non interactif.",
        "value": false
      }
    ],
    "htmlCode": "<button type=\"{{type}}\" aria-label=\"{{ariaLabel}}\" class=\"inline-flex items-center justify-center ds-gap-2xsmall ds-px-medium ds-py-2xsmall ds-rounded-small ds-bg-brand-primary ds-text-neutral-white ds-shadow-middle-short ds-font-button ds-font-weight-medium ds-text-size-default transition-colors duration-200 ease-out hover:ds-bg-brand-500 focus:outline focus:ds-outline-system-focused focus:ds-outline-width_medium active:ds-bg-brand-900 disabled:ds-bg-neutral-inactive disabled:ds-text-neutral-light-grey disabled:cursor-not-allowed {{fullWidthClass}}\"><span class=\"ds-mr-2xsmall\">{{iconLeft}}</span><span>{{label}}</span><span class=\"ds-ml-2xsmall\">{{iconRight}}</span></button>",
    "interactions": [
      {
        "action": "hover",
        "description": "Le fond s’éclaircit avec le token ds-bg-brand-500 (semantics.colors.backgrounds.brand.brand-500) pour un feedback visuel."
      },
      {
        "action": "focus",
        "description": "Un contour de focus est affiché via ds-outline-system-focused (semantics.system.focused) avec ds-outline-width_medium (2px) pour l’accessibilité clavier."
      },
      {
        "action": "click",
        "description": "À l’activation, le fond passe en ds-bg-brand-900 (semantics.colors.backgrounds.brand.brand-900) pour signifier l’état pressé."
      },
      {
        "action": "disabled",
        "description": "Le bouton utilise ds-bg-neutral-inactive (semantics.colors.backgrounds.neutrals.neutral_inactive) et ds-text-neutral-light-grey (semantics.neutrals.neutral_light-grey), le curseur devient not-allowed et les états hover/focus sont inactifs."
      }
    ],
    "subComponents": []
  }
  }
